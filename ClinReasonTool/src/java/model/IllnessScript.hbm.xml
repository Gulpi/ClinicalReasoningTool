<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="net.clinicalReasoning.IllnessScript">

	<class name="IllnessScript" table="casus.illnessscript">
		<cache usage="nonstrict-read-write" />
		<comment>Illness Script</comment>	
	
		<id name="id" type="long" column="id">
			<generator class="sequence">
				<param name ="sequence">CASUS.CR_SEQ</param>
			</generator>
		</id>
		
			
		<property name="name" column="name" type="string" length="300"/>
		<property name="courseOfTime" column="courseoftime" type="int"/>	
		
</class>
</hibernate-mapping>